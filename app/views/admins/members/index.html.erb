<div classs="container">
	<div class="row">
		<div class="col-md-7 col-md-offset-3">
		  <h2>会員一覧</h2>
		  <table class="table table-hover table-inverse">
		    <thead>
		      <tr>
		  	    <th>会員ID</th>
		  	    <th>氏名</th>
		  	    <th>メールアドレス</th>
		  	    <th>ステータス</th>
		      </tr>
		    </thead>
		    <tbody>
		   <% @members.each do |member| %>
		  	  <tr>
		  	    <td><%= member.id %></td>
		  	    <td><%= member.first_name %></td>
		  	    <td><%= member.email %></td>
		  	    <td></td>
		      </tr>
		      <% end %>
		    </tbody>
		</table>
	</div>
</div>