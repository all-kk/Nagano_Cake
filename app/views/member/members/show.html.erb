<div classs="container">
	<div class="row">
		<div class="col-md-7 col-md-offset-3">
      <h2>マイページ</h2>
      <h3>登録情報<%= link_to "編集する", edit_member_member_path, class: "btn btn-primary" %><%= link_to "パスワードを変更する", edit_member_password_path, class: "btn btn-primary" %></h3>
      <table class="table table-bordered ">
        <tr>
          <td class="active">氏名</td>
          <td><%= @member.first_name %>
            <%= @member.last_name %>
          </td>
        </tr>

        <tr>
          <td class="active">カナ</td>
          <td><%= @member.first_name_phonetic %>
            <%= @member.last_name_phonetic %>
          </td>
        </tr>

        <tr>
          <td class="active">郵便番号</td>
          <td><%= @member.postcode %></td>
        </tr>

        <tr>
          <td class="active">住所</td>
          <td><%= @member.address %></td>
        </tr>

        <tr>
          <td class="active">電話番号</td>
          <td><%= @member.telephone_number %></td>
        </tr>

        <tr>
          <td class="active">メールアドレス</td>
          <td><%= @member.email %></td>
        </tr>
      </tbody>
    </table>
      <h3>配送先<div class="button_wrapper"><%= link_to "一覧を見る", member_shippings_path, class: "btn btn-primary" %>
      </div>
      </h3>
      <h3>注文履歴一覧<div class="button_wrapper"><%= link_to "一覧を見る", member_orders_path, class: "btn btn-primary" %>
      </div>
    </h3>
</div>
</div>
  </div>

</div>
</div>