<div class="shipping_edit_area" style="margin-top: 100px;">
  <div class="container">
    <div class="row">
    <h2 style="font-family: HiraMinPro-W6;">配送先編集</h2>
    <div class="col-xs-8">
        <table class="table table-bordered">
                    <%= form_for @shipping, url:member_shipping_path(@shipping), method: :patch do |f| %>
               <tr>
              	  <th><%= f.label :郵便番号 %></th>
                  <th><%= f.text_field :postcode, autofocus: true, autocomplete: "postcode" %></th>
                  <th></th>
               </tr>
               <tr>
                  <th><%= f.label :住所 %></th>
                  <th><%= f.text_field :address, autofocus: true, autocomplete: "address" %></th>
                  <th></th>
               </tr>
               <tr>
                  <th><%= f.label :宛名 %></th>
                  <th><%= f.text_field :name, autofocus: true, autocomplete: "name" %></th>
                  <th><%= f.submit '編集する', class: "btn btn-primary" %></th>
               </tr>
                    <% end %>
         </table>
       </div>
    </div>
  </div>
</div>